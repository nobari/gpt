{
  "name": "chatgpt",
  "version": "0.0.1",
  "description": "ChatGPT API playground.",
  "main": "index.ts",
  "scripts": {
    "build": "tsc && npm run scss",
    "dev": "npm-run-all --parallel live tscWatch scssWatch",
    "compile": "tsc && scss",
    "live": "live-server --port=3000 --open=docs/index.html",
    "scssWatch": "sass --watch src/scss/style.scss:docs/css/style.css",
    "scss": "sass src/scss/style.scss:docs/css/style.css",
    "tscWatch": "tsc -w"
  },
  "keywords": [
    "ChatGPT",
    "API",
    "Sasha",
    "Sapata"
  ],
  "author": "Sadegh Nobari",
  "license": "MIT",
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/marked": "^4.0.8",
    "fs": "^0.0.1-security",
    "npm-run-all": "^4.1.5",
    "sass": "^1.59.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "crypto-js": "^4.1.1",
    "marked": "^4.2.12"
  }
}
